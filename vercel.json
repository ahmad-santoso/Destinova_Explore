{
    "version": 2,
    "name": "destinova_explore",
    "builds": [
      {
        "src": "public/landingpage.blade.php",
        "use": "@vercel/php"
      },
      {
        "src": "resources/js/*",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "public/index.php"
      }
    ]
  }
  